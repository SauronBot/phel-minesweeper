{
    "name": "sauronbot/phel-minesweeper",
    "description": "Terminal Minesweeper written in Phel â€” a Lisp that compiles to PHP.",
    "keywords": ["phel", "lisp", "game", "minesweeper", "terminal", "cli"],
    "homepage": "https://github.com/SauronBot/phel-minesweeper",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "phel-lang/phel-lang": "dev-main"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "PhelGenerated\\": "src/PhelGenerated"
        }
    },
    "scripts": {
        "play": "vendor/bin/phel run src/main.phel",
        "build": "vendor/bin/phel build --no-cache",
        "format": "vendor/bin/phel format",
        "test": "vendor/bin/phel test",
        "repl": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phel repl"
        ]
    }
}
